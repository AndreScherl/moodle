<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="locals/mbslicenseinfo/db" VERSION="20160426" COMMENT="XMLDB file for Moodle locals/mbslicenseinfo"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_mbslicenseinfo_fmeta" COMMENT="Additional meta information for files.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="title" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Title of file, e.g. Mona Lisa"/>
        <FIELD NAME="source" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Source/URL of file."/>
        <FIELD NAME="files_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Id of file in files table."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="files_id" TYPE="foreign" FIELDS="files_id" REFTABLE="files" REFFIELDS="id" COMMENT="Primary key of files table."/>
      </KEYS>
    </TABLE>
    <TABLE NAME="local_mbslicenseinfo_ul" COMMENT="User licenses of local mbslicenseinfo.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="shortname" TYPE="char" LENGTH="13" NOTNULL="false" SEQUENCE="false" COMMENT="Id of this table record as string like 'ul_123'"/>
        <FIELD NAME="fullname" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Title of the license."/>
        <FIELD NAME="source" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Source url of the license description."/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Id of the user who created the license."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" COMMENT="Id of user table"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>