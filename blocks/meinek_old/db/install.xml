<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/meinek_old/db" VERSION="20120904" COMMENT="XMLDB file for Moodle blocks/meinek_old"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_meinek_old_userprefs" COMMENT="Saved user preferences for the selection form">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="id" NEXT="sortby"/>
        <FIELD NAME="sortby" TYPE="char" LENGTH="50" NOTNULL="false" SEQUENCE="false" PREVIOUS="userid" NEXT="coursevisited"/>
        <FIELD NAME="coursevisited" TYPE="char" LENGTH="50" NOTNULL="false" SEQUENCE="false" PREVIOUS="sortby" NEXT="numcourses"/>
        <FIELD NAME="numcourses" TYPE="int" LENGTH="5" NOTNULL="true" UNSIGNED="true" DEFAULT="5" SEQUENCE="false" PREVIOUS="coursevisited"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="idxuserid" UNIQUE="false" FIELDS="userid"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>